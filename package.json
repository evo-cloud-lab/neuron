        "async": "0.2.9",
        "msgpack": "0.1.11"
    },
    "devDependencies": {
        "mocha": "1.12.0",
        "sandboxed-module": "0.2.1",
        "blanket": "1.1.6"
    },
    "engines": {
        "node": ">=0.10",
        "npm": "1"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "https://github.com/evo-cloud/neuron/raw/master/LICENSE"
        }
    ],
    "config": {
        "blanket": {
            "onlyCwd": true,
            "pattern": "//^((?!node_modules).)*/lib/.+$/"
        }
